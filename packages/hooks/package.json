    "test": "mocha --config ../../.mocharc.json --recursive test/**.test.ts test/**/*.test.ts"
  },
  "directories": {
    "lib": "lib"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@types/chai": "^4.2.14",
